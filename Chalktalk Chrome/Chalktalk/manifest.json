{
  "manifest_version": 2,
  "name": "Chalktalk",
  "description": "Chalktalk.",
  "version": "1",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["start.js"]
    }
  },
  "permissions": [
    "http://localhost:11235/",
    "webview",
    "system.display"
  ],
    "sockets": {
      "udp": {
        "bind" : "*",
        "send" : "*"
      }
    }
}
